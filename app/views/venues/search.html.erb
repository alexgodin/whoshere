<% @herenow.each do |i| %>
<img src="<%= i["user"]["photo"] %>" class="pic" /><br>
<%= i["user"]["firstName"] %><br>
<% end %>
This room is <%= @place.room_id.to_s %>
<script type="text/javascript" charset="utf-8">
  $(document).ready(
    function(){
      setInterval(function(){
        $.get("<%= '/chat/' + @place.room_id.to_s %>", function(data){
          console.log(data)
          $('#chat').html(data)
        })
      }, 5000);
    });
</script>
<div id="chat">
</div>


